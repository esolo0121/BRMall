			<div class="mall-input">
				<form class="layui-form" action="">
					<div class="layui-form-item">
						<label class="layui-form-label">会员昵称</label>
						<div class="layui-input-block">
							<input type="text" value="" name="title" lay-verify="title" autocomplete="off" class="layui-input">
						</div>
					</div>

					<div class="layui-form-item pub-seclect">
						<label class="layui-form-label">会员等级</label>
						<div class="layui-input-inline">
							<select name="modules" lay-verify="level" lay-search="">
								<option value=""></option>
								<option value="1">超级管理员地方</option>
								<option value="2">超级管理员22</option>
								<option value="3">超级管理员44</option>
							</select>

						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">用户积分</label>
						<div class="layui-input-block higher-place">
							<span>0.88888</span>积分							
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">用户积分</label>
						<div class="layui-input-block higher-place">
							<span>0.88888</span>积分							
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">手机号码</label>
						<div class="layui-input-block">
							<input type="tel" name="phoneN" lay-verify="phoneN" autocomplete="off" class="layui-input Phone-input">
							<p class="explain-p">前台登陆账号，手机号和邮箱任意一项都可以</p>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">邮箱</label>
						<div class="layui-input-block">
							<input type="text" name="email" lay-verify="emailTest" autocomplete="off" class="layui-input Email-input">
							<p class="explain-p">前台登陆账号，手机号和邮箱任意一项都可以</p>
						</div>
					</div>
				
					<div class="layui-form-item">
						<label class="layui-form-label">新密码</label>
						<div class="layui-input-block">
							<input type="password" name="fphone" lay-verify="passW" autocomplete="off" value="" class="layui-input newPass">
							<p class="explain-p">留空表示不修改密码</p>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">确认密码</label>
						<div class="layui-input-block">
							<input type="password" name="fphone" lay-verify="fphone" autocomplete="off" value="" class="layui-input newPass">
							<p class="explain-p">留空表示不修改密码</p>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">上级分销用户</label>
						<div class="layui-input-block">
							<input type="text" name="fphone" lay-verify="fphone" autocomplete="off" value="" class="layui-input">
							<p class="explain-p">请正确填写会员的登录账号（手机号）</p>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">性别</label>
						<div class="layui-input-block">
							<input type="radio" name="ejeh" value="保密" title="保密" checked="">
							<input type="radio" name="ejeh" value="男" title="男">
							<input type="radio" name="ejeh" value="女" title="女">

						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">QQ号码</label>
						<div class="layui-input-block">
							<input type="text" name="title2" lay-verify="title2" autocomplete="off" value="" class="layui-input">
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">注册时间</label>
						<div class="layui-input-block higher-place">
							2017-07-21 14:14						
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">冻结会员</label>
						<div class="layui-input-block">
						 <input type="checkbox" name="close" lay-skin="switch" lay-text="开启|关闭">
							<p class="explain-p">请正确填写会员的登录账号（手机号）</p>
						</div>
					</div>
					<div class="layui-form-item">
						<label class="layui-form-label">分销功能</label>
						<div class="layui-input-block">
						 <input type="checkbox" checked="" name="open" lay-skin="switch" lay-filter="switchTest" lay-text="开启|关闭">
							<p class="explain-p">请正确填写会员的登录账号（手机号）</p>
						</div>
					</div>
					<div class="layui-form-item member-table">
						<label class="layui-form-label">分销功能</label>
						<div class="layui-input-block">
					     <div class="layui-form overview-table">

								<table class="layui-table" lay-even="" lay-skin="row">
									<tbody>
										<tr>
											<td class="first">用户余额</td>
											<td>汉族</td>
											<td class="first">更新时间:</td>
											<td>1989-10-14</td>

										</tr>
										<tr>
											<td class="first">上一级编号</td>
											<td>汉族</td>
											<td class="first">更新时间:</td>
											<td>1989-10-14</td>
										</tr>
										<tr>
											<td class="first">程序版本:</td>
											<td>汉族</td>
											<td class="first">更新时间:</td>
											<td>1989-10-14</td>
										</tr>
										<tr>
											<td class="first">服务器操作系统::</td>
											<td>汉族</td>
											<td class="first">更新时间:</td>
											<td>1989-10-14</td>
										</tr>
										

									</tbody>
								</table>
							</div>
						
							
						</div>
					</div>

					<div class="layui-inline">
						<button class="layui-btn" lay-submit="" lay-filter="demo2">确认提交</button>
					</div>
				</form>

			</div>
		</div>

		<script typet="text/javascript" src="../static/layui/layui.js"></script>
		<script typet="text/javascript" src="../static/js/jquery-1.9.0.js"></script>
		<script typet="text/javascript" src="../static/js/frame.js"></script>
		<script typet="text/javascript" src="../static/js/page-all.js"></script>
		<script>
			layui.use(['form', 'upload', 'element', 'layedit', 'laydate'], function() {
				var $ = layui.jquery,
					layer = layui.layer,
					layedit = layui.layedit,
					laydate = layui.laydate,
					form = layui.form(), //获取form模块
					upload = layui.upload; //Tab的切换功能，切换事件监听等，需要依赖element模块

				layui.upload({
					url: '', //上传接口

					success: function(res) { //上传成功后的回调
						console.log(res)
					}
				});

				layui.upload({
					url: '/test/upload.json',
					elem: '#test', //指定原始元素，默认直接查找class="layui-upload-file"

					method: 'get', //上传接口的http类型

					success: function(res) {
						LAY_demo_upload.src = res.url;
					}
				});

				//创建一个编辑器
				var editIndex = layedit.build('LAY_demo_editor');
				var regP = /^1[3578]\d{9}$/;
				var regE = /^[A-Za-z0-9\u4e00-\u9fa5]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
				//自定义验证规则
				form.verify({
					title: function(value) {
						if(value.length < 1) {
							return '会员昵称不能为空';
						}
					},
					passW:function(value){
						if(value!=$(".newPass").eq(1).val()){
							
							return '两次密码不一致';
						}
					},
					level: function(value) {
						if(value.length < 1) {
							return '会员等级不能为空';
						}
					},
					phoneN: function(value) {

						var val2 = $(".Email-input").val();

						if(regP.test(value) || regE.test(val2)) {

						} else {
							return '手机号码和邮箱格式都有错误';
						}
					},

					emailTest: function(value) {
						var val1 = $(".Phone-input").val();

						if(regE.test(value) || regP.test(val1)) {

						} else {
							return '手机号码和邮箱格式都有错误';
						}
					},
					pass: [/(.+){6,12}$/, '密码必须6到12位'],
					content: function(value) {
						layedit.sync(editIndex);
					},

				});

				//监听提交
				form.on('submit(demo2)', function(data) {
					layer.alert(JSON.stringify(data.field), {

					})
					return false;
				});

			});
		</script>
	</body>

</html>