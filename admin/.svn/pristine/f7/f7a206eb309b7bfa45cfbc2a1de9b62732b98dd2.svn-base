
body{word-wrap:break-word;background-color: #ffffff}
body,ol,ul,h1,h2,h3,h4,h5,h6,p,th,td,dl,dd,form,fieldset,legend,input,textarea,select,iframe{margin:0;padding:0;}
li{list-style:none;}
a,u,del,s{text-decoration:none;color:#000;}
a,img,button,iframe{border:0;}
i,em{font-style:normal;}
b,th,strong{font-weight:normal;}
img{vertical-align:middle;} 

input,textarea,select{outline:none;}
i,span{display: inline-block;}

.cu{cursor:pointer;}

.clearx{clear:both;}
.over{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}

.fl{float:left;}
.fr{float:right;}

.pr{position: relative;}
.pa{position: absolute;}



a:focus {
	outline: none;
}

.mall-input .layui-textarea:hover {
	border: 1px solid #009688 !important;
}

.wrapper{
	width: 98%;
	padding: 1%;
	margin-bottom: 100px;
}

.layui-input{
	padding-right: 10px;
}
.public-tittle{
	
	padding-bottom: 8px;
	border-bottom: solid 1px #E6E6E6;
	margin-bottom: 10px;	
	overflow: hidden;
	
}


.public-tittle p{
	font: 16px/20px "微软雅黑";
	color: #333;
}

.public-tittle span{
	font: 12px/18px "微软雅黑";
	color: #999;
}

.tip-tittle{
	display: block;
	color:#ff7b86;
	font: 14px/30px "微软雅黑";
	margin-left: 5px;
}

.public-tip{
	background: #FFE7E9;
	padding-bottom: 10px;
}
.tip-tittle i{
	margin-right: 10px;
}

.tip-list{
	margin-left: 20px;
	font: 12px/24px "微软雅黑";
	color: #555;
}

.tip-list p span{
	color:#70DAC6 ;
	margin-right: 5px;
}

.main-tittle{
	
	height: 24px;
	margin: 15px 0 10px;
}

.tittle-left span{
	font: 16px/16px "微软雅黑";
	color: #333;
}

.tittle-left u{
	font: 12px/16px "微软雅黑";
	color: #999;
	margin: 0 15px 0 6px;
}

.tittle-left i{
	cursor: pointer;
	color: #999;
	font:700 14px/14px "微软雅黑";
}

.tittle-left i:hover{
	color: #E95055;
}

.head-t th{
	font-weight: 700;
}


.edit-btn{
	display:inline-block;
	border:solid 1px #ff3c3c;
	font: 14px/30px "微软雅黑";
	height: 30px;	
	color:#ff3c3c;
	padding: 0 10px;
	border-radius: 4px;
	margin-right: 10px;
}
.edit-btn:hover{
	background: #FFE7E9;
}

.edit-btn .icon-tianjia{
	font-weight: 700;
}

.form-box{
	padding-top: 10px;
	border-top: 1px solid #e2e2e2;
}




.site-demo-button .lay-change{
	font: 14px/28px "微软雅黑";
	height: 28px; 
	padding: 0 10px;
}


.lay-change a{
	color: #fff;
}

.come-back{
	margin-right: 15px;
}
.come-back i{
	font: 40px/40px "微软雅黑";
	color: #999;
}

.come-back i:hover{
	color: #009688;
}

/*权限资源列表*/
.resource-list .layui-input-inline{
	margin-right: 10px;
}
.tittle-right .layui-input{
	height: 24px;
	border: 1px solid #009688 !important;
	line-height: 24px;
}

.tittle-right .layui-input:hover{
	border: 1px solid #009688 !important;
}

.resource-list .layui-form-select dl dd{
	height: 24px;
	line-height: 24px;
}

.resource-list .layui-form-select dl{
	top: 25px;
}


/*跳转操作*/
.skip-bg{
	   background: url(../images-temporary/skip-bg.png) no-repeat center;
    background-size: cover;
    background-position: center;
      width: 100%;
    height: 100%;
}
.skip-html{
	   width: 100%;
    height: 100%;
}

.skip-main{
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -170px;
	margin-top: -128px;	
	text-align: center;
}

.skip-top{
	background: #fff;
	padding: 30px 50px;
	width: 340px;
	text-align: center;
	background-color: rgba(255, 255, 255, 0.4);
}
.skip-tittle{
	overflow: hidden;
    display: inline-block;	
}
.skip-tittle .icon-duihao{
	color: #009688;
	font: 45px/50px "微软雅黑";
	margin-right: 10px;
	float: left;
}

.word-box{
	font: 16px/50px "微软雅黑";
	color: #666;
	float: left;
}

.skip-time{
	font: 16px/30px "微软雅黑";
}
.skip-time span{
	color: #3c8dbc;
}

.skip-time u{
	margin-left: 20px;
}
.skip-a{
	font: 16px/50px "微软雅黑";
}
.skip-a a{
	margin-right: 20px;
	color: #3c8dbc;
}

.skip-bottom{
	text-align: center;
	margin-top: 20px;
	color: #666;
}

.skip-bottom a{
	color: #3c8dbc;
}



/*公共搜索框*/
.search-pubox .layui-input{
	width: 200px;
}

.search-pubox .layui-btn{
	height: 24px;
	line-height: 24px;
	margin-left: -2px;
	padding: 0 12px;
}


